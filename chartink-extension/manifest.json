{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Chart ink tools",
    "icons": {
        "512": "tool-box.png"
    },
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "SM Tools"
      },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}